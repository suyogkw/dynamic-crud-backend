{
  "reference": "students",
  "queries": {
    "DATA": {
      "query": "select * from students",
      "params": []
    },
    "UNIQUE_CLAUSE": {
      "query": "select * from students where name=? and college=?",
      "params": [ "{server.create_name}", "{client.college}" ]
    },
    "IDENTITY_CLAUSE": {
      "query": "select * from students where id=?",
      "params": [ "{client.id}" ]
    },
    "INSERT": {
      "query": "insert into students values((select isnull(max(id), 0) + 1 from students),?,?,?,?,?)",
      "params": [ "{server.create_name}", "{client.age}", "{client.address}", "{client.college}", "{server.timestamp}" ]
    },

    "UPDATE": {
      "query": "update students set address = ?, college = ?, updated = ? where id = ?",
      "params": [ "{client.address}", "{client.college}", "{server.timestamp}", "{client.id}" ]
    },
    "UPDATE_VERIFY": {
      "query": "select * from students where name=? and address=? and college=?",
      "params": [ "{client.name}", "{client.address}", "{client.college}" ]
    },
    "DELETE": {
      "query": "delete from students where id = ?",
      "params": [ "{client.id}" ]
    },
    "HISTORY": {
      "query": "insert into students_history (action,student_id,name,age,address,college,updated,updatedby,hostname) values(?,?,?,?,?,?,?,?,?)",
      "params": [ "{server.action}", "{data.id}", "{data.name}", "{data.age}", "{data.address}", "{data.college}", "{server.timestamp}", "{server.context_user}", "{server.hostname}" ]
    }    
  }
}